rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are permissive and are meant for development only.
    // They allow any authenticated user to read and write to any document.
    // You should replace these with more secure rules before production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
